{
  "name": "bus-clean-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "serve:prod": "ng serve --configuration production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "jest",
    "prepush": "npm run lint | npm run test",
    "prettier": "prettier --write .",
    "generate-api": "openapi-generator-cli generate -i http://digisped-app-srv:90/swagger/v1/swagger.json -g typescript-angular -o src/shared/api --additional-properties=npmName=api,ngVersion=20.2.0 | prettier"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "endOfLine": "crlf",
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.8",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/material": "^20.2.8",
    "@angular/platform-browser": "^20.2.0",
    "@angular/pwa": "^20.2.1",
    "@angular/router": "^20.2.0",
    "@angular/service-worker": "^20.2.0",
    "@sc/system-web-client": "file:.yalc/@sc/system-web-client",
    "ngx-cookie-service": "^20.1.1",
    "rxjs": "~7.8.0",
    "swiper": "^12.0.3",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^20.2.0",
    "@angular-eslint/eslint-plugin-template": "^20.2.0",
    "@angular-eslint/template-parser": "^20.2.0",
    "@angular/build": "^20.2.0",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.2.0",
    "@eslint/js": "^9.34.0",
    "@openapitools/openapi-generator-cli": "2.25.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "angular-eslint": "^20.4.0",
    "eslint": "^9.33.0",
    "eslint-plugin-jsdoc": "^54.1.1",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "^15.0.0",
    "ng-mocks": "^14.13.5",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  }
}
